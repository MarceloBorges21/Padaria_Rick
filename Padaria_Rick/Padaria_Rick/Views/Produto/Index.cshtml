
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Cardapio</h2>
<br />
<table class="table table-dark">
    <thead>
        <tr>           
            <th scope="col">Nome</th>           
        </tr>
    </thead>
    <tbody>
        @foreach (var pessoa in Model)
        {
            <tr>              
                <td>@pessoa.Nome</td>              
            </tr>
        }
    </tbody>
</table>


